<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
</head>
<body>
  <h2>Web Chat Project</h2>
  <form action="">
    <input data-testid="nickname-box" type="text" id="nickname-box" placeholder="Choose a new Nickname"/>
    <button id="nickname-button" data-testid="nickname-button" type="button">Change My Nickname</button>
  <div>
    <h4>Users Online:</h4>
    <ul id="users-on"></ul>
  </div>
  <div>
    <ul id="message-list"></ul>
  </div>
  <div>
    <input data-testid="message-box" type="text" id="message-box"  placeholder="Type your message here" />
    <button type="button" data-testid="send-button" id="send-button">Send</button>
  </div>
  </form>
  <script src="/socket.io/socket.io.js"></script>
  <script src="./js/chat.js"></script>
</body>
</html>